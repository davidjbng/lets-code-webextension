{
  "manifest_version": 2,
  "name": "My Webextension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "lego.png"
  },
  "browser_action": {
    "default_icon": "lego.png",
    "default_title": "My Webextension",
    "default_popup": "popup.html"
  }
}
